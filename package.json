{
  "name": "nuxmv-syntax",
  "displayName": "nuXmv Syntax Highlighting",
  "description": "VSCode syntax highlighting for nuXmv",
  "version": "0.2.0",
  "publisher": "dario-tortorici",
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "license": "SEE LICENSE IN LICENSE.txt",
    "languages": [
      {
        "id": "nuxmv",
        "aliases": [
          "nuXmv",
          "nuxmv"
        ],
        "extensions": [
          ".smv"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "nuxmv",
        "path": "./snippets/nuxmv.json"
      }
    ],
    "grammars": [
      {
        "language": "nuxmv",
        "scopeName": "source.nuxmv",
        "path": "./syntaxes/nuxmv.tmLanguage.json"
      }
    ]
  },
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:nuxmv"
  ],
  "homepage": "https://github.com/DarioTortorici//nuxmv-vscode-syntax",
  "repository": {
    "type": "git",
    "url": "https://github.com/DarioTortorici//nuxmv-vscode-syntax"
  },
  "bugs": {
    "url": "https://github.com/DarioTortorici//nuxmv-vscode-syntax/issues"
  }
}